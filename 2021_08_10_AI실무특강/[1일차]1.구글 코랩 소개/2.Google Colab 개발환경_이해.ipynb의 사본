\n","    }\n","  "],"text/plain":["<IPython.core.display.Javascript object>"]},"metadata":{"tags":[]}},{"output_type":"display_data","data":{"application/javascript":["download(\"download_93b87ead-2187-4c89-a90e-b91dee0126da\", \"test.txt\", 5)"],"text/plain":["<IPython.core.display.Javascript object>"]},"metadata":{"tags":[]}}]},{"cell_type":"code","metadata":{"colab":{"resources":{"http://localhost:8080/nbextensions/google.colab/files.js":{"data":"","ok":true,"headers":[["content-type","application/javascript"]],"status":200,"status_text":""}},"base_uri":"https://localhost:8080/","height":76},"id":"QwPdaGQkwNRL","executionInfo":{"status":"ok","timestamp":1628577021039,"user_tz":-540,"elapsed":23043,"user":{"displayName":"박세익","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14Gi3-OyGBTMf3S4RVqWXB_fGZosw_1o0lrpJmmfe=s64","userId":"13867642966478677924"}},"outputId":"ce59795f-be0a-4155-ac79-ac18bcf567f8"},"source":["from google.colab import files\n","upload = files.upload()"],"execution_count":6,"outputs":[{"output_type":"display_data","data":{"text/html":["\n","     <input type=\"file\" id=\"files-6f2f2963-4e97-4bde-8f72-26d8c8d4c5d1\" name=\"files[]\" multiple disabled\n","        style=\"border:none\" />\n","     <output id=\"result-6f2f2963-4e97-4bde-8f72-26d8c8d4c5d1\">\n","      Upload widget is only available when the cell has been executed in the\n","      current browser session. Please rerun this cell to enable.\n","      </output>\n","      <script src=\"/nbextensions/google.colab/files.js\"></script> "],"text/plain":["<IPython.core.display.HTML object>"]},"metadata":{"tags":[]}},{"output_type":"stream","text":["Saving 줌 강의 방식.txt to 줌 강의 방식.txt\n"],"name":"stdout"}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":35},"id":"8um_whOzwU6o","executionInfo":{"status":"ok","timestamp":1628577035905,"user_tz":-540,"elapsed":323,"user":{"displayName":"박세익","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14Gi3-OyGBTM